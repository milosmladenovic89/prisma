generator client {
  provider = "prisma-client-js"
  output   = "../generated/client1"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Coils {
  id       Int      @id @default(autoincrement())
  number   String   @unique
  order    String
  width    Int
  thick    Int
  createAt DateTime @default(now())
}
