generator client {
  provider = "prisma-client-js"
  output   = "../generated/client3"
}

datasource db3 {
  provider = "mysql"
  url      = env("DATABASE_URL3")
}

model Coils3 {
  id          Int      @id @default(autoincrement())
  status      Boolean
  numberCoils Int
  number      String   @unique
  order       String
  width       Int
  thick       Int
  createAt    DateTime @default(now())
}
